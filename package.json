{
    "name": "nuxt-shopify",
    "version": "0.0.1",
    "license": "MIT",
    "type": "module",
    "description": "Easily integrate shopify into your nuxt project ðŸš€",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/konkonam/nuxt-shopify.git"
    },
    "exports": {
        ".": {
            "types": "./dist/types.d.ts",
            "import": "./dist/module.mjs",
            "require": "./dist/module.cjs"
        }
    },
    "main": "./dist/module.cjs",
    "types": "./dist/types.d.ts",
    "files": [
        "dist"
    ],
    "scripts": {
        "prepare": "nuxt-module-build prepare && nuxt-module-build build --stub",
        "prepare:dev": "pnpm prepare && pnpm run --filter=./playgrounds/playground cleanup",
        "build": "nuxt-module-build build",
        "build:docs": "vitepress build docs",
        "dev": "nuxi dev playgrounds/playground",
        "dev:docs": "vitepress dev docs",
        "dev:v4": "nuxi dev playgrounds/playground-nuxt-v4",
        "preview:docs": "vitepress preview docs",
        "lint": "eslint .",
        "lint:types": "nuxi typecheck",
        "cleanup": "pnpm run --filter=./playgrounds/playground cleanup && nuxi cleanup",
        "postinstall": "pnpm run prepare && pnpm run prepare:dev"
    },
    "dependencies": {
        "@nuxt/kit": "^3.14.159"
    },
    "devDependencies": {
        "@types/node": "latest",
        "@nuxt/eslint-config": "^0.6.1",
        "@nuxt/module-builder": "^0.8.4",
        "@nuxt/schema": "^3.14.159",
        "@nuxt/test-utils": "^3.14.4",
        "@shopify/admin-api-client": "^1.0.4",
        "@shopify/api-codegen-preset": "^1.1.2",
        "@shopify/storefront-api-client": "^1.0.3",
        "@fontsource-variable/sono": "^5.1.0",
        "@graphql-codegen/cli": "^5.0.3",
        "eslint": "^9.14.0",
        "nuxt": "^3.14.159",
        "vitepress": "^1.5.0",
        "vitest": "^2.1.4"
    }
}
